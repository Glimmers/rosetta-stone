    "Services": {
      "Installed to": {
        "Windows Server": [
          "<p><code>Get-Service\n</code></p>"
        ],
        "Red Hat Enterprise Linux": [
          "<p><code>/etc/system\n</code></p>"
        ],
        "Debian": [
          "<p><code>/etc/system\n</code></p>"
        ],
        "Ubuntu": [
          "<p><code>/etc/init.d\n</code></p>"
        ],
        "OS X": [
          "<p><code>/System/Library/LaunchDaemons/\n</code></p>"
        ],
        "OpenBSD": [
          "<p><code>?\n</code></p>"
        ],
        "SmartOS": [
          "<p><code>?\n</code></p>"
        ]
      },
      "List Services": {
        "Windows Server": [
          "<p><code>?\n</code></p>"
        ],
        "Red Hat Enterprise Linux": [
          "<p><code>systemctl list-unit-files --type=service\n</code></p>"
        ],
        "Debian": [
          "<p><code>systemctl list-unit-files --type=service\n</code></p>"
        ],
        "Ubuntu": [
          "<p><code>ls /etc/init.d\n</code></p>"
        ],
        "OS X": [
          "<p><code>launchctl list\n</code></p>"
        ],
        "OpenBSD": [
          "<p><code>?\n</code></p>"
        ],
        "SmartOS": [
          "<p><code>svcs\n</code></p>",
          "<p><a href=\"https://wiki.smartos.org/display/DOC/SmartOS+Technical+FAQs\">Reference</a></p>"
        ]
      },
      "Start Service": {
        "Windows Server": [
          "<p><code>?\n</code></p>"
        ],
        "Red Hat Enterprise Linux": [
          "<p><code>systemctl start servicename\n</code></p>"
        ],
        "Debian": [
          "<p><code>systemctl start servicename\n</code></p>"
        ],
        "Ubuntu": [
          "<p><code>/etc/init.d/[service file] start\n</code></p>"
        ],
        "OS X": [
          "<p><code>launchctl load -w /System/Library/LaunchDaemons/ssh.plist\n</code></p>"
        ],
        "OpenBSD": [
          "<p><code>?\n</code></p>"
        ],
        "SmartOS": [
          "<p><code>svcadm enable [service name]\n</code></p>",
          "<p><a href=\"https://wiki.smartos.org/display/DOC/SmartOS+Technical+FAQs\">Reference</a></p>"
        ]
      },
      "Stop Service": {
        "Windows Server": [
          "<p><code>Stop-Service [servicename]\n</code></p>"
        ],
        "Red Hat Enterprise Linux": [
          "<p><code>systemctl stop [servicename]\n</code></p>"
        ],
        "Debian": [
          "<p><code>systemctl stop [servicename]\n</code></p>"
        ],
        "Ubuntu": [
          "<p><code>/etc/init.d/[service file] stop\n</code></p>"
        ],
        "OS X": [
          "<p><code>launchctl unload -w /System/Library/LaunchDaemons/ssh.plist\n</code></p>"
        ],
        "OpenBSD": [
          "<p><code>pkill [process name]\n</code></p>"
        ],
        "SmartOS": [
          "<p><code>svcadm disable\n</code></p>",
          "<p><a href=\"https://wiki.smartos.org/display/DOC/SmartOS+Technical+FAQs\">Reference</a></p>"
        ]
      },
      "Restart Service": {
        "Windows Server": [
          "<p><code>Restart-Service [servicename]\n</code></p>"
        ],
        "Red Hat Enterprise Linux": [
          "<p><code>systemctl restart [servicename]\n</code></p>"
        ],
        "Debian": [
          "<p><code>systemctl restart [servicename]\n</code></p>"
        ],
        "Ubuntu": [
          "<p><code>/etc/init.d/[service file] restart\n</code></p>"
        ],
        "OS X": [
          "<p><code>?\n</code></p>"
        ],
        "OpenBSD": [
          "<p><code>?\n</code></p>"
        ],
        "SmartOS": [
          "<p><code>svcadm restart\n</code></p>",
          "<p><a href=\"https://wiki.smartos.org/display/DOC/SmartOS+Technical+FAQs\">Reference</a></p>"
        ]
      }
    },
    "Packaging": {
      "Install package file": {
        "Windows Server": [
          "<p><code>msiexec /package [msi file] /quiet\n</code></p>",
          "<p><a href=\"https://msdn.microsoft.com/en-us/library/aa372024%28v=vs.85%29.aspx\">Reference</a></p>"
        ],
        "Red Hat Enterprise Linux": [
          "<p><code>yum install [package name]\n</code></p>",
          "<p>or if the file is already downloaded:\n    rpm -Uvh [rpm file]</p>"
        ],
        "Debian": [
          "<p><code>dpkg -i [deb file]\n</code></p>"
        ],
        "Ubuntu": [
          "<p><code>dpkg -i [deb file]\n</code></p>"
        ],
        "OS X": [
          "<p><code>?\n</code></p>"
        ],
        "OpenBSD": [
          "<p><code>pkg_add\n</code></p>"
        ],
        "SmartOS": [
          "<p><code>pkg_add\n</code></p>"
        ]
      },
      "Uninstall package": {
        "Windows Server": [
          "<p><code>$app = Get-WmiObject -Class Win32_Product | Where-Object {\n    $_.Name -match \"Software Name\"\n}\n$app.Uninstall()\n</code></p>"
        ],
        "Red Hat Enterprise Linux": [
          "<p><code>yum remove [package name]\n</code></p>",
          "<p>or</p>",
          "<p><code>rpm -e [package name]\n</code></p>"
        ],
        "Debian": [
          "<p><code>apt-get remove [package name]\n</code></p>"
        ],
        "Ubuntu": [
          "<p><code>apt-get remove [package name]\n</code></p>"
        ],
        "OS X": [
          "<p><code>?\n</code></p>"
        ],
        "OpenBSD": [
          "<p><code>?\n</code></p>"
        ],
        "SmartOS": [
          "<p><code>?\n</code></p>"
        ]
      },
      "List all installed packages": {
        "Windows Server": [
          "<p><code>Get-ItemProperty HKLM:\\Software\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\* | select DisplayName, Publisher, InstallDate\n</code></p>",
          "<p><a href=\"http://blogs.technet.com/b/heyscriptingguy/archive/2013/11/15/use-powershell-to-find-installed-software.aspx\">TechNet</a></p>"
        ],
        "Red Hat Enterprise Linux": [
          "<p><code>rpm -qa\n</code></p>"
        ],
        "Debian": [
          "<p><code>dpkg -l\n</code></p>"
        ],
        "Ubuntu": [
          "<p><code>dpkg -l\n</code></p>"
        ],
        "OS X": [
          "<p><code>?\n</code></p>"
        ],
        "OpenBSD": [
          "<p><code>pkg_info\n</code></p>"
        ],
        "SmartOS": [
          "<p><code>?\n</code></p>"
        ]
      },
      "Describe what an installed package is": {
        "Windows Server": [
          "<p><code>?\n</code></p>"
        ],
        "Red Hat Enterprise Linux": [
          "<p><code>rpm -qi [package name]\n</code></p>"
        ],
        "Debian": [
          "<p><code>dpkg --status pkgname\n</code></p>"
        ],
        "Ubuntu": [
          "<p><code>dpkg --status pkgname\n</code></p>"
        ],
        "OS X": [
          "<p><code>?\n</code></p>"
        ],
        "OpenBSD": [
          "<p><code>pkg_info\n</code></p>"
        ],
        "SmartOS": [
          "<p><code>?\n</code></p>"
        ]
      },
