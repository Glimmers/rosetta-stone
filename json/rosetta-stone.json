        "Debian": [
          "<p><code>less /proc/meminfo\n</code></p>"
        ],
        "Ubuntu": [
          "<p><code>less /proc/meminfo\n</code></p>"
        ],
        "OS X": [
          "<p><code>system_profiler SPMemoryDataType\n</code></p>"
        ],
        "OpenBSD": [
          "<p><code>?\n</code></p>"
        ],
        "SmartOS": [
          "<p><code>sysinfo\n</code></p>",
          "<p><a href=\"https://smartos.org/man/1m/sysinfo\">Manual</a></p>"
        ]
      }
