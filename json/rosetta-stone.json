          "<p><code>/etc/system\n</code></p>"
        ],
        "Debian": [
          "<p><code>/etc/system\n</code></p>"
        ],
        "Ubuntu": [
          "<p><code>/etc/init.d\n</code></p>"
        ],
        "OS X": [
          "<p><code>/System/Library/LaunchDaemons/\n</code></p>"
        ],
        "OpenBSD": [
          "<p><code>?\n</code></p>"
        ],
        "SmartOS": [
          "<p>User installed</p>",
          "<p><code>/opt/custom/smf\n</code></p>",
          "<p>Pkgsrc installed</p>",
