        "Red Hat Enterprise Linux": [
          "<p><code>systemctl restart [servicename]\n</code></p>"
        ],
        "Debian": [
          "<p><code>systemctl restart [servicename]\n</code></p>"
        ],
        "Ubuntu": [
          "<p><code>/etc/init.d/[service file] restart\n</code></p>"
        ],
        "OS X": [
          "<p><code>?\n</code></p>"
        ],
        "OpenBSD": [
          "<p><code>?\n</code></p>"
        ],
        "SmartOS": [
          "<p><code>svcadm restart [service name]\n</code></p>",
