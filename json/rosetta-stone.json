{
  "sections": {
    "Shell basics": {
      "Logged in user account": {
        "Windows Server": [
          "<p><code>$env:username\n</code></p>"
        ],
        "Red Hat Enterprise Linux": [
          "<p><code>whoami\n</code></p>"
        ],
        "Debian": [
          "<p><code>whoami\n</code></p>"
        ],
        "Ubuntu": [
          "<p><code>whoami\n</code></p>"
        ],
        "OS X": [
          "<p><code>whoami\n</code></p>"
        ],
        "OpenBSD": [
          "<p><code>whoami\n</code></p>"
        ],
        "SmartOS": [
          "<p><code>?\n</code></p>"
        ]
      },
      "Host name": {
        "Windows Server": [
          "<p><code>$env:COMPUTERNAME\n</code></p>"
        ],
        "Red Hat Enterprise Linux": [
          "<p><code>hostname\n</code></p>"
